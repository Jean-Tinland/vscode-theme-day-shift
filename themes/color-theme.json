    "titleBar.activeBackground": "#fafbfc",
    "titleBar.border": "#e1e4e8"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Escape characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Named entities",
      "scope": "entity.name",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Function names",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e0b972"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#2fc2c3"
      }
    },
    {
      "name": "Static types",
      "scope": ["entity.name.type", "storage.type.cs"],
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "CSS class selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#2fc2c3"
      }
    },
    {
      "name": "CSS transition name",
      "scope": "support.type.property-name.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#e0b972"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "fontStyle": "",
        "foreground": "#e0b972"
      }
    },
    {
      "name": "CSS colors",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "SCSS include & mixins params",
      "scope": ["meta.at-rule.mixin", "meta.at-rule.include"],
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "SCSS include & mixins parenthesis",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round",
        "punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#39465e"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "CSS constant",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "CSS units",
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#39465e"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#1e2737"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2fc2c3"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#74829b"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Markup link descriptions",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Provided (support) values",
      "scope": "support",
      "settings": {
        "fontStyle": "",
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Provided functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "Provieded variables",
      "scope": "support.variable",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "CSS property names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#e0b972"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Embedded template punctuation",
      "scope": "string.template meta.embedded",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Tag brackets",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Key/value separators",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#74829b"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#e78482"
      }
    }
  ]
}
