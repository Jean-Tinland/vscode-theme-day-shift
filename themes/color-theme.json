    "editor.selectionHighlightBorder": "#1e273740",
    "editor.wordHighlightBackground": "#d1ab6638",
    "editor.wordHighlightStrongBackground": "#d1ab6670",
    "editorBracketMatch.background": "#f1f8ff",
