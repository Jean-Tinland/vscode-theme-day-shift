    "list.inactiveFocusBackground": "#1e273713",
    "list.inactiveSelectionBackground": "#1e273713",
    "list.inactiveSelectionForeground": "#1e2737",
    "list.invalidItemForeground": "#d1ab66",
    "list.warningForeground": "#d1ab66",
    "scrollbar.shadow": "#00000000",
    "sideBar.background": "#fafbfc",
    "sideBar.border": "#e1e4e8",
    "sideBar.foreground": "#1e2737",
    "sideBarSectionHeader.background": "#f1f2f3",
    "sideBarSectionHeader.foreground": "#1e2737",
    "sideBarTitle.foreground": "#1e2737",
    "searchEditor.findMatchBackground": "#6c6f931a",
    "statusBar.background": "#ffffff",
    "statusBar.border": "#e1e4e8",
    "statusBar.debuggingBackground": "#fafbfc",
    "statusBar.debuggingForeground": "#1e2737",
    "statusBar.foreground": "#1e2737",
    "statusBar.noFolderBackground": "#fafbfc",
    "statusBar.noFolderForeground": "#1e2737",
    "statusBarItem.hoverBackground": "#e1e4e8",
    "statusBarItem.remoteForeground": "#1e2737",
    "statusBarItem.remoteBackground": "#d1ab66",
    "statusBarItem.remoteHoverForeground": "#1e2737",
    "statusBarItem.remoteHoverBackground": "#e1e4e8",
    "tab.activeBackground": "#ffffff",
    "tab.activeBorder": "#fafbfc",
    "tab.activeBorderTop": "#1e2737",
    "tab.border": "#e1e4e8",
    "tab.inactiveBackground": "#fafbfc",
    "tab.inactiveForeground": "#586069",
    "terminal.ansiBrightRed": "#fb918f",
    "terminal.ansiBrightGreen": "#a1ded0",
    "terminal.ansiBrightYellow": "#e0b972",
    "terminal.ansiBrightBlue": "#78c3df",
    "terminal.ansiBrightMagenta": "#bf91e0",
    "terminal.ansiBrightCyan": "#35d8d9",
    "terminal.ansiRed": "#e78482",
    "terminal.ansiGreen": "#8fc8bb",
    "terminal.ansiYellow": "#d1ab66",
    "terminal.ansiBlue": "#6db3ce",
    "terminal.ansiMagenta": "#ad82cb",
    "terminal.ansiCyan": "#2fc2c3",
    "titleBar.activeBackground": "#fafbfc",
    "titleBar.border": "#e1e4e8"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#98a8c5"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "constant.numeric.bin.python",
        "storage.type.number.python"
      ],
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Escape characters",
      "scope": ["constant.character.escape", "constant.language.python"],
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Named entities",
      "scope": [
        "entity.name",
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "meta.function-call.arguments.python",
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.end.python"
      ],
      "settings": {
        "foreground": "#39465e",
        "fontStyle": ""
      }
    },
    {
      "name": "Function names",
      "scope": [
        "entity.name.function",
        "entity.name.type.class.python",
        "meta.function-call.python",
        "support.function.magic.python"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#2fc2c3"
      }
    },
    {
      "name": "Tag attributes & Python exeptions",
      "scope": ["entity.other.attribute-name", "support.type.exception.python"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "CSS class selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#74829b"
      }
    },
    {
      "name": "CSS transition name",
      "scope": "support.type.property-name.css",
      "settings": {
        "fontStyle": "",
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "CSS ID selectors",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "fontStyle": "",
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "CSS colors",
      "scope": "constant.other.color.rgb-value.hex.css",
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "SCSS include & mixins params",
      "scope": ["meta.at-rule.mixin", "meta.at-rule.include"],
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "SCSS include & mixins parenthesis",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round",
        "punctuation.definition.parameters.end.bracket.round"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#39465e"
      }
    },
    {
      "name": "Inherited classes",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "CSS constant",
      "scope": "constant.numeric.css",
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "CSS units",
      "scope": "keyword.other.unit",
      "settings": {
        "fontStyle": "",
        "foreground": "#39465e"
      }
    },
    {
      "name": "Pseudo CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Variable names",
      "scope": ["entity.name.variable", "variable"],
      "settings": {
        "foreground": "#1e2737"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type.function.python",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Special operators",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup quotes",
      "scope": "markup.quote",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6db3ce"
      }
    },
    {
      "name": "Markup headings",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup italics",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#2fc2c3"
      }
    },
    {
      "name": "Markup code",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#e78482"
      }
    },
    {
      "name": "Markup links",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "Storage keywords",
      "scope": "storage",
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted",
        "string.template",
        "storage.type.string.python",
        "string.regexp.quoted.single.python"
      ],
      "settings": {
        "foreground": "#74829b"
      }
    },
    {
      "name": "Object/JSON property names",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "(S)CSS support functions",
      "scope": ["support.function.misc.scss", "support.function.misc.css"],
      "settings": {
        "foreground": "#8fc8bb"
      }
    },
    {
      "name": "Language variables",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#d1ab66"
      }
    },
    {
      "name": "CSS property names, Tag brackets, Key/value separators, Embedded template punctuation, Parameters, Regex, Markup link descriptions",
      "scope": [
        "support.type.property-name.css",
        "punctuation.definition.tag",
        "punctuation.separator",
        "string.template meta.embedded",
        "variable.parameter",
        "string.regexp",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#39465e"
      }
    },
    {
      "name": "Template expressions",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#74829b"
      }
    },
    {
      "name": "Embedded section punctuation",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ad82cb"
      }
    },
    {
      "name": "Markup list punctuation",
      "scope": "punctuation.definition.list",
      "settings": {
        "foreground": "#e78482"
      }
    }
  ]
}
